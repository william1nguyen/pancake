{
  "name": "typescript-template-tsup",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "TS backend template powered by tsup",
  "author": "William Nguyen",
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{ts, tsx, js,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf ./dist",
    "dev": "npm run build && npm start",
    "build": "tsup",
    "start": "node --enable-source-maps dist/index.js",
    "db:migrate": "tsx db-migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "watch": "tsup --watch",
    "watch:start": "tsup --watch --onSuccess \"npm start\"",
    "lint": "npx @biomejs/biome check src",
    "lint:fix": "npx @biomejs/biome check --write src",
    "test": "vitest"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "sourcemap": true,
    "clean": true,
    "shims": true
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/prompt": "^17.7.1",
    "@types/node": "^18.17.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.22.8",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "tsup": "^7.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@bull-board/api": "^6.5.3",
    "@bull-board/fastify": "^6.5.3",
    "@fastify/basic-auth": "^6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/type-provider-typebox": "^5.0.1",
    "@sentry/node": "^7.63.0",
    "@sentry/tracing": "^7.63.0",
    "@sinclair/typebox": "^0.33.17",
    "@types/jsonwebtoken": "^9.0.7",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.7",
    "bullmq": "^5.34.1",
    "compiler": "link:@sinclair/typebox/compiler",
    "date-fns": "^4.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.31.2",
    "fastify": "^5.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kleur": "^4.1.5",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "proxy-chain": "^2.5.4",
    "puppeteer": "^23.7.0",
    "puppeteer-core": "^23.7.0",
    "puppeteer-page-proxy": "^1.3.0"
  }
}
